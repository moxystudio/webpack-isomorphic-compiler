{
  "name": "webpack-isomorphic-compiler",
  "version": "0.3.0",
  "description": "A compiler that makes your life easier if you are building isomorphic webpack powered apps, that is, single page applications with server-side rendering",
  "keywords": [
    "webpack",
    "isomorphic",
    "server-side",
    "rendering",
    "render",
    "compiler",
    "compilation"
  ],
  "author": "Andr√© Cruz <andre@moxy.studio>",
  "license": "MIT",
  "bugs": "https://github.com/moxystudio/webpack-isomorphic-compiler/issues",
  "homepage": "https://github.com/moxystudio/webpack-isomorphic-compiler",
  "repository": {
    "type": "git",
    "url": "git@github.com:moxystudio/webpack-isomorphic-compiler"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "test": "jest --env node --coverage && rimraf test/tmp",
    "test:watch": "jest --env node --watch",
    "posttest": "npm run lint",
    "precommit": "npm run lint:staged"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "dependencies": {
    "indent-string": "^3.1.0",
    "lodash.merge": "^4.6.0",
    "lodash.wrap": "^4.1.1",
    "p-finally": "^1.0.0",
    "p-settle": "^2.0.0",
    "pretty-error": "^2.1.0",
    "webpack": "^2.4.1"
  },
  "devDependencies": {
    "delay": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-moxy": "^2.0.2",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "lint-staged": "^3.4.0",
    "lodash.escaperegexp": "^4.1.2",
    "p-try": "^1.0.0",
    "pify": "^2.3.0",
    "rimraf": "^2.6.1",
    "strip-ansi": "^3.0.1"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "test/tmp/",
      "test/configs/"
    ],
    "coveragePathIgnorePatterns": [
      "test/"
    ]
  }
}
